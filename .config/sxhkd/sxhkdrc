
# Volume Buttons

XF86AudioRaiseVolume
	amixer sset Master 1+ unmute

XF86AudioLowerVolume
	amixer sset Master 1- unmute

XF86AudioMute
	amixer sset Master toggle

# Media Buttons

XF86AudioNext
	mocp -f

XF86AudioPlay
	  ~/.moc/autorun.sh

XF86AudioPrev
	mocp -r

# Brightness Buttons

XF86MonBrightnessUp
	light -A 10

XF86MonBrightnessDown
	light -U 10

# Keyobard Brightness Buttons

XF86KbdBrightnessUp
	kbdlight up 20

XF86KbdBrightnessDown
	kbdlight down 20
